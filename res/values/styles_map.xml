<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="tabview_map_Style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/map_top_repeat</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">@dimen/pixels_15</item>
        <item name="android:paddingRight">@dimen/pixels_15</item>
    </style>    
    <!-- 头部 -->
    <style name="FormTop_BtnView_map">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginTop">@dimen/pixels_12</item>
        <item name="android:layout_marginRight">@dimen/pixels_15</item>
        <item name="android:background">#00000000</item>
        <item name="android:src">@drawable/map_bdsign</item>
    </style>
    <style name="FormTop_Button_map">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/pixels_10</item>
        <item name="android:layout_marginRight">@dimen/pixels_12</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>  
    
    <style name="FormTop_TextView_map">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/topTextSize</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">horizontal</item>
    </style>   
    <!-- mapicon -->    
    
        
